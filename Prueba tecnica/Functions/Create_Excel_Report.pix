try
{
    // 1. Leer el contenido del archivo en modo binario
    byte[] fileContent = System.IO.File.ReadAllBytes(StrReportFilePath);

    // 2. Instancia un objeto HttpClient
    using (var client = new System.Net.Http.HttpClient())
    {
        // 3. Configura los encabezados (headers) de la petición
        client.DefaultRequestHeaders.Authorization = new System.Net.Http.Headers.AuthenticationHeaderValue("Bearer", AccessToken);

