        // 5. Envía la petición y obtiene la respuesta de forma síncrona
        //    Usamos .Result para esperar a que la tarea asíncrona termine
        var response = client.SendAsync(request).Result;

        // 6. Verifica si la respuesta fue exitosa
        response.EnsureSuccessStatusCode();

        // 7. Lee el contenido de la respuesta de forma síncrona
        responseContent = response.Content.ReadAsStringAsync().Result;
        isSuccess = true;
